<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="https://www.openriskmanual.org/example"
         xmlns="https://www.openriskmanual.org/example#">

<owl:Ontology rdf:about="https://www.openriskmanual.org/example">
  <owl:versionIRI rdf:resource="https://www.openriskmanual.org/example/1.0"/>
  <rdfs:label>Borrower Ontology (For Open Risk Academy Course DAT31063, Class Inheritance in Data Science)</rdfs:label>
</owl:Ontology>

<owl:DatatypeProperty rdf:about="#annual_income">
  <rdfs:domain rdf:resource="#PrivateBorrower"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  <rdfs:label>annual income</rdfs:label>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#market_capitalization">
  <rdfs:domain rdf:resource="#CorporateBorrower"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  <rdfs:label>market capitalization</rdfs:label>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#name">
  <rdfs:domain rdf:resource="#Borrower"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <rdfs:label>name</rdfs:label>
</owl:DatatypeProperty>

<owl:Class rdf:about="#PrivateBorrower">
  <rdfs:subClassOf rdf:resource="#Borrower"/>
  <rdfs:label>Private Borrower</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#CorporateBorrower">
  <rdfs:subClassOf rdf:resource="#Borrower"/>
  <owl:disjointWith rdf:resource="#PrivateBorrower"/>
  <rdfs:label>Corporate Borrower</rdfs:label>
</owl:Class>

